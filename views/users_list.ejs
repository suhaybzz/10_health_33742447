<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Users - HealthTrack</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <h1>Registered Users</h1>
    <p><a href="/">Home</a></p>

    <table>
      <thead>
        <tr>
          <th>Username</th>
          <th>Name</th>
          <th>Email</th>
          <th>Registered</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(function(u) { %>
          <tr>
            <td><%= u.username %></td>
            <td><%= (u.first_name || '') + ' ' + (u.last_name || '') %></td>
            <td><%= u.email %></td>
            <td><%= u.created_at.toISOString().slice(0,10) %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
